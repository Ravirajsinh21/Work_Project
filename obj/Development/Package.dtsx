using System;
using Microsoft.SqlServer.Dts.Pipeline.Wrapper;
using Microsoft.SqlServer.Dts.Runtime.Wrapper;
#endregion

[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    /// <summary>
    /// Called once before rows begin processing.
    /// Use this for initialization if needed.
    /// </summary>
    public override void PreExecute()
    {
        base.PreExecute();
    }

    /// <summary>
    /// Called after all rows have been processed.
    /// Use this for cleanup if needed.
    /// </summary>
    public override void PostExecute()
    {
        base.PostExecute();
    }

    /// <summary>
    /// Processes each row passing through the component.
    /// </summary>
    /// <param name="Row">The row currently being processed.</param>
    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        // Process each column and assign cleaned values back to the row
        Row.Age = ProcessNumericColumn(Row.Age, "0");
        Row.UserID = ProcessNumericColumn(Row.UserID, "0");
        Row.FullName = ProcessStringColumn(Row.FullName);
        Row.Email = ProcessEmailColumn(Row.Email);
        Row.RegistrationDate = ProcessDateColumn(Row.RegistrationDate);
        Row.LastLoginDate = ProcessDateColumn(Row.LastLoginDate);
        Row.PurchaseTotal = ProcessPurchaseTotal(Row.PurchaseTotal.ToString()).ToString();
    }

    /// <summary>
    /// Processes numeric columns by removing invalid entries.
    /// </summary>
    /// <param name="columnValue">The value to be processed.</param>
    /// <param name="defaultValue">The default value for invalid entries.</param>
    /// <returns>Processed numeric value as a string.</returns>
    private string ProcessNumericColumn(string columnValue, string defaultValue)
    {
        string cleanedValue = columnValue.Trim().Trim('"');
        if (string.IsNullOrWhiteSpace(cleanedValue) || !int.TryParse(cleanedValue, out int number) || number <= 0)
        {
            return defaultValue;
        }
        return number.ToString();
    }

    /// <summary>
    /// Processes email columns with basic validation.
    /// </summary>
    /// <param name="columnValue">The value to be processed.</param>
    /// <returns>Validated email or "NA" for invalid entries.</returns>
    private string ProcessEmailColumn(string columnValue)
    {
        string cleanedValue = columnValue?.Trim().Trim('"') ?? string.Empty;
        if (string.IsNullOrEmpty(cleanedValue) || !cleanedValue.Contains("@") || !cleanedValue.Contains("."))
        {
            return "NA";
        }
        return cleanedValue;
    }

    /// <summary>
    /// Processes string columns by removing unnecessary characters.
    /// </summary>
    /// <param name="columnValue">The value to be processed.</param>
    /// <returns>Processed string or "NA" for empty entries.</returns>
    private string ProcessStringColumn(string columnValue)
    {
        string cleanedValue = columnValue?.Trim().Trim('"') ?? string.Empty;
        return string.IsNullOrEmpty(cleanedValue) ? "NA" : cleanedValue;
    }

    /// <summary>
    /// Processes date columns by converting to a standard format.
    /// </summary>
    /// <param name="columnValue">The value to be processed.</param>
    /// <returns>Formatted date or default date for invalid entries.</returns>
    private string ProcessDateColumn(string columnValue)
    {
        // Trim surrounding quotes and whitespace
        string cleanedValue = columnValue.Trim().Trim('"');

        // Define minimum valid date
        DateTime minDate = new DateTime(2000, 1, 1);

        // Parse the cleaned value to a DateTime
        if (DateTime.TryParse(cleanedValue, out DateTime dateValue))
        {
            // Replace future dates with the current date
            if (dateValue > DateTime.Now)
            {
                return DateTime.Now.ToString("yyyy-MM-dd");
            }
            // Replace very old dates with a default value
            else if (dateValue < minDate)
            {
                return "2001-01-01"; // Default date for out-of-range values
            }
            else
            {
                return dateValue.ToString("yyyy-MM-dd"); // Return formatted date
            }
        }
        else
        {
            return "2001-01-01"; // Default date for invalid input
        }
    }


    /// <summary>
    /// Processes purchase total with default value and range check.
    /// </summary>
    /// <param name="columnValue">The value to be processed.</param>
    /// <returns>Processed purchase total.</returns>
    private float ProcessPurchaseTotal(string columnValue)
    {
        // Check if the input is null or empty
        if (string.IsNullOrEmpty(columnValue))
        {
            return 0f; // Default value for empty input
        }

        // Clean the input value
        string cleanedValue = columnValue.Trim().Trim('"');

        // Try parsing the cleaned value to float
        if (float.TryParse(cleanedValue, out float parsedValue))
        {
            // Return 0 if the value is greater than or equal to 1000
            return parsedValue >= 1000 ? 0f : parsedValue;
        }
        else
        {
            // Return 0 for invalid numeric input
            return 0f;
        }
    }

